<!DOCTYPE html>
<html>
<head>
  <title>Emotion - Share your feelings, help others.</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <header>
    <div class="navbar">
      <div class="container">
        <%= render 'layouts/navigation' %>
      </div>
    </div>
  </header>
  <div id="main" role="main">  
      <div class="container">
        <div class="row">
          <div class="span12">    
            <%= render 'layouts/messages' %>
            <%= yield %>
          </div>
        </div>
      </div>
    <footer>
    </footer>      
  </div>
</body>
</html>
